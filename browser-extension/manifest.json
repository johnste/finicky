{
  "name": "Finicky",
  "description": "The Official Finicky Browser Extension. Requires Finicky (macOS only)",
  "version": "0.1",
  "manifest_version": 2,
  "homepage_url": "https://github.com/johnste/finicky",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" 
  },
  "permissions": ["contextMenus", "<all_urls>"]  
}
